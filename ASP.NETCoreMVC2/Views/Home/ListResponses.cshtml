@model IEnumerable<Guests>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Ответы гостей</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css"/>
</head>
<body>
    <div class="panel-body">
        <h2>Здесь список людей, кто будет присутствовать на вечеринке</h2>
        <table class="table table-sm table-striped table-bordered">
            <thead>
                <tr>
                    <th>Имя</th>
                    <th>Емаил</th>
                    <th>Телефон</th>
                </tr>
            </thead>
            <tbody>
                @foreach (Guests r in Model)
                {
                    <tr>
                        <td>@r.Name</td>
                        <td>@r.Email</td>
                        <td>@r.Phone</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
